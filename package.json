{
  "name": "micro-dev",
  "version": "1.1.1",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "test": "xo"
  },
  "bin": {
    "micro-dev": "./bin/micro-dev.js"
  },
  "repository": "zeit/micro-dev",
  "license": "MIT",
  "xo": {
    "ignores": [
      "examples/**/*"
    ],
    "extends": "prettier",
    "rules": {
      "unicorn/no-process-exit": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run test",
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "keywords": [
    "micro",
    "development",
    "service",
    "microservice",
    "serverless",
    "API"
  ],
  "dependencies": {
    "anymatch": "1.3.2",
    "boxen": "1.2.1",
    "chalk": "2.0.1",
    "chokidar": "1.7.0",
    "clipboardy": "1.1.4",
    "get-port": "3.1.0",
    "ip": "1.1.5",
    "jsome": "2.3.26",
    "micro": "8.0.1",
    "mri": "1.1.0",
    "pkg-up": "2.0.0",
    "pretty-error": "2.1.1",
    "string-length": "2.0.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "2.3.0",
    "husky": "0.14.3",
    "lint-staged": "4.0.2",
    "prettier": "1.5.3",
    "xo": "0.18.2"
  }
}
